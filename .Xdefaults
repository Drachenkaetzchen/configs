URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1
URxvt.depth: 32
URxvt.saveLines: 200000
URxvt.scrollBar: false
URxvt.transparent: true
URxvt.shading: 10

URxvt*scrollBar:         false
URxvt*secondaryScroll:    true
URxvt*cursorBlink:       true
!URxvt*cursorUnderline:   true
!URxvt*urgentOnBell:       true
!!URxvt*override-redirect:false
!!URxvt*borderLess:       false
!!URxvt*internalBorder:       0
!!URxvt*externalBorder:       0

URxvt*fonts.0:     xft:Terminus:pixelsize=11:antialias=false
URxvt*fonts.1:     xft:Terminus:pixelsize=16:antialias=false
URxvt*fonts.2:     xft:Terminus:pixelsize=20:antialias=false
URxvt*fonts.3:     xft:Terminus:pixelsize=25:antialias=false

!URxvt*boldFont:     xft:Terminus:bold:pixelsize=11:antialias=false

URxvt.keysym.Mod4-plus: perl:fonts:plus
URxvt.keysym.Mod4-minus: perl:fonts:minus

URxvt.perl-ext: fonts
URxvt.perl-lib: /home/felicitus/.urxvt/


URxvt.internalBorder: 1

! jwr low contrast

!black
*color0: #222222
*color8: #454545
!red
*color1: #9E5641
*color9: #CC896D
!green
*color2: #6C7E55
*color10: #C4DF90
!yellow
*color3: #CAAF2B
*color11: #FFE080
!blue
*color4: #3B5F8F
*color12: #B8DDEA
!magenta
*color5: #956D9D
*color13: #C18FCB
!cyan
*color6: #4c8ea1
*color14: #296B77
!white
*color7: #c2c2c2
*color15: #dddddd

URxvt*borderColor: #343434
URxvt*background: #303030
URxvt*foreground: #cdcdcd
!Cursor
URxvt*cursorColor: #DCDCCC

!URxvt*background:  #3f3f3f
!URxvt*foreground:  #dcdccc
! black + red
!URxvt*color0:      #1E2320
!URxvt*color1:      #705050
! green + yellow
!URxvt*color2:      #60b48a
!URxvt*color3:      #df5e5e
! blue + purple
!URxvt*color4:      #506070
!URxvt*color5:      #dc8cc3
! cyan + white
!URxvt*color6:      #8cd0d3
!URxvt*color7:      #ececec
! bright-black + bright-red
!URxvt*color8:      #709080
!URxvt*color9:      #dca3a3
! bright-green + bright-yellow
!URxvt*color10:     #c3bf9f
!URxvt*color11:     #f0dfaf
! bright-blue + bright-purple
!URxvt*color12:     #94bff3
!URxvt*color13:     #ec93d3
! bright-cyan + bright-white
!URxvt*color14:     #93e0e3
!URxvt*color15:     #ffffff
